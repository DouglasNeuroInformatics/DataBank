{
  "name": "databank",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "env-cmd turbo run build",
    "clean": "fd -H -I --prune 'dist|node_modules' --exec rm -rf",
    "dev": "env-cmd turbo run dev",
    "lint": "turbo run lint",
    "test": "env-cmd turbo run test",
    "ws": "env-cmd bash -c 'yarn workspace @databank/$0 ${@:1}'"
  },
  "dependencies": {
    "ts-pattern": "^5.0.1",
    "type-fest": "^3.12.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@databank/types": "*",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "globals": "^13.20.0",
    "prettier": "^2.8.8",
    "turbo": "^1.10.6",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "client",
    "server",
    "types"
  ]
}
