    "@types/nodemon": "^1.19.6",
    "@types/supertest": "^6.0.2",
    "esbuild": "^0.24.0",
    "esbuild-plugin-tsc": "^0.4.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.21.1",
    "supertest": "^7.0.0",
    "tsx": "4.19.2",
    "unplugin-swc": "^1.5.1"
